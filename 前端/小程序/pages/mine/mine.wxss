page{
  background: #EEF1F6;
}
.head_content{
    width: 100%;
    background:#23DB85;
    height:200rpx;
    position: relative;
}
.user_content{
    display:flex;
    flex-direction: row;
    align-items: center;
}
.user_image_content{
    margin-left:60rpx;
    width:128rpx;
    height:128rpx;
    padding:4rpx;
    background:#FFF;
    border-radius:50%;
}
.icon_image{
    width:100%;
    height:100%;
    border-radius:50%;
}
.user_name_content{
    margin-left:20rpx;
}
.name_title{
    color:#FFF;
    font-size:32rpx;
    font-weight:600;
}
.message_content{
    width:48rpx;
    height:48rpx;
    margin-left:50%;
}
.message_image{
    width:100%;
    height:100%;
}
.content{
   position: relative;
   background: #EEF1F6;
   width: 100%;
}
.list_content{
    margin:0 5%;
    border-color: #FFF;
    position: relative;
    width: 90%;
    top: -20rpx;
    border-radius: 14rpx;
    overflow: hidden;
}
