<view class="list_content">
  <view class="cells cells_after-title">
    <navigator url="{{link}}" class="cell cell_access" wx-if="{{isItem_1}}">
      <view class="cell__bd">{{title}}</view>
      <view class="cell__ft cell__ft_in-access"></view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active" wx-if="{{isItem_2}}">
      <view class="weui-cell__bd">{{title}}</view>
      
      <view class="weui-cell__ft weui-cell__ft_in-access">{{info}}</view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active" wx-if="{{isItem_3}}">
      <view class="weui-cell__bd">{{title}}</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">
        <image src="{{icon}}" style="vertical-align: middle;width:60rpx; height: 60rpx;border-radius:50%">
        </image>
      </view>

    </navigator>
  </view>
</view>